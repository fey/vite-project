{
  "type": "module",
  "name": "vite-project",
  "version": "1.0.0",
  "description": "This project was bootstrapped with Fastify-CLI.",
  "main": "app.js",
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "fastify": "^5.0.0",
    "fastify-plugin": "^5.0.0",
    "@fastify/autoload": "^6.0.0",
    "@fastify/sensible": "^6.0.0",
    "fastify-cli": "^7.4.1",
    "valtio": "^2.3.0",
    "vite": "^7.3.1"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "npm-check-updates": "^19.5.0"
  },
  "scripts": {
    "test": "node --test test/**/*.test.js",
    "start": "fastify start -l info app.js",
    "dev:backend": "fastify start -w -l info -P app.js --ignore-watch='node_modules .git logs/error.log vite.config.js frontend'",
    "dev:frontend": "vite",
    "dev": "concurrently 'npm:dev:*' -c cyan,yellow",
    "build": "vite build",
    "preview": "vite preview"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fey/vite-project.git"
  },
